{
  "name": "@edro/backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "prebuild": "node -e \"console.log('Skipping build clean step')\"",
    "build": "echo \"Skipping TypeScript compilation\"",
    "start": "node -e \"console.log('[start] backend start override'); require('ts-node/register/transpile-only'); require('./src/index.ts')\"",
    "job:harvest:test": "ts-node --transpile-only src/jobs/harvest-test.ts",
    "job:process-harvest": "ts-node --transpile-only src/jobs/process-harvest.ts",
    "job:generate-drops": "ts-node --transpile-only src/jobs/generate-drops.ts",
    "job:extract-blueprints": "ts-node --transpile-only src/jobs/extract-blueprints.ts",
    "job:generate-drops-v2": "ts-node --transpile-only src/jobs/generate-drops-v2.ts",
    "job:rag-feeder": "ts-node --transpile-only src/jobs/rag-feeder.ts",
    "job:daily-plan-preview": "ts-node --transpile-only src/jobs/daily-plan-preview.ts",
    "job:bancas-watcher": "ts-node --transpile-only src/jobs/bancas-watcher.ts",
    "job:pdf-validator": "ts-node --transpile-only src/jobs/pdf-validator.ts",
    "db:migrate": "ts-node --transpile-only src/db/migrate.ts",
    "db:seed": "ts-node --transpile-only src/db/seed.ts",
    "db:check": "node check-migrations.js",
    "test": "ts-node src/tests/adaptiveEngine.test.ts"
  },
  "dependencies": {
    "@fastify/compress": "^6.5.0",
    "@fastify/cors": "^9.0.0",
    "@fastify/jwt": "^6.0.0",
    "@sentry/node": "^7.100.0",
    "@types/node": "^20.0.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^4.15.0",
    "cheerio": "^1.1.2",
    "js-yaml": "^4.1.0",
    "dotenv": "^16.4.0",
    "fastify": "^4.25.0",
    "fastify-plugin": "^5.1.0",
    "ioredis": "^5.3.0",
    "pdf-parse": "^1.1.1",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.0",
    "openai": "^4.52.0",
    "pg": "^8.11.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "zod": "^3.22.2",
    "@aws-sdk/client-s3": "^3.614.0",
    "@aws-sdk/s3-request-presigner": "^3.614.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.15.6",
    "ts-node-dev": "^2.0.0"
  }
}
