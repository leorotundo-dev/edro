{
  "name": "@edro/backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "prebuild": "node -e \"console.log('Skipping build clean step')\"",
    "build": "echo \"Skipping TypeScript compilation\"",
    "start": "node -e \"console.log('[start] backend start override'); require('ts-node/register/transpile-only'); require('./src/index.ts')\"",
    "db:migrate": "ts-node --transpile-only src/db/migrate.ts"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.0",
    "@fastify/jwt": "^6.0.0",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.4.0",
    "fastify": "^4.25.0",
    "nodemailer": "^6.9.15",
    "openai": "^4.52.0",
    "pg": "^8.11.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.15.6",
    "ts-node-dev": "^2.0.0"
  }
}
