{
  "name": "@edro/backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "prebuild": "node -e \"console.log('Skipping build clean step')\"",
    "build": "echo \"Skipping TypeScript compilation\"",
    "start": "node -e \"console.log('[start] backend start override'); require('ts-node/register/transpile-only'); require('./src/index.ts')\"",
    "db:migrate": "ts-node --transpile-only src/db/migrate.ts",
    "calendar:generate": "ts-node --transpile-only src/scripts/generateEdroCalendarMaster.ts",
    "calendar:import": "ts-node --transpile-only src/scripts/importCalendarCsv.ts",
    "calendar:recalc": "ts-node --transpile-only src/scripts/recalcCalendarRelevance.ts"
  },
  "dependencies": {
    "@edro/shared": "workspace:*",
    "@aws-sdk/client-s3": "^3.975.0",
    "@fastify/cors": "^9.0.0",
    "@fastify/jwt": "^6.0.0",
    "@fastify/multipart": "^8.2.0",
    "@fastify/rate-limit": "^8.1.0",
    "@mozilla/readability": "^0.5.0",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "csv-parse": "^5.6.0",
    "dotenv": "^16.4.0",
    "fastify": "^4.25.0",
    "jsdom": "^23.0.0",
    "libsodium-wrappers": "^0.7.13",
    "mammoth": "^1.8.0",
    "mime-types": "^2.1.35",
    "nodemailer": "^6.9.15",
    "openai": "^4.52.0",
    "openid-client": "^6.0.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.11.0",
    "prom-client": "^15.1.0",
    "redis": "^4.6.13",
    "rss-parser": "^3.13.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.0.0",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.15.6",
    "ts-node-dev": "^2.0.0"
  }
}
